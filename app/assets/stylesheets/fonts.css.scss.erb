
@font-face {
    font-family: 'alexbrush-regular';
    // src: font-url("/assets/AlexBrush-Regular.ttf") format('truetype');
    src: url(<%= asset_path 'alexbrush-regular.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'alexbrush-regular.woff'%>) format('woff'),
         url(<%= asset_path 'alexbrush-regular.ttf'%>)  format('truetype'),
         url(<%= asset_path 'alexbrush-regular.svg#svgFontName'%>) format('svg');

    font-weight: normal;
    font-style: normal;
}